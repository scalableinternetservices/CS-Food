<h1>Items</h1>

<table>
  <thead>
  <tr>
    <th>Name</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @items.each do |item| %>
      <tr>
        <td><%= item.name %></td>
        <td><%= link_to 'Show', item %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>
<%= link_to 'Back', items_path %>